	keywords = {Antitrust and Trade Regulation, Business, Digital Markets Act, Digital Services Act, E-Commerce, {EU} law, Economics, Ex ante rules, Industrial Organization, Interoperability, Law, Platforms, Social and Behavioral Sciences, Tech Competition, keller-platform-reg-syl-2020},
}

