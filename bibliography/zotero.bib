
@article{hanratty_local_2017,
	title = {Do Local Economic Conditions Affect Homelessness? Impact of Area Housing Market Factors, Unemployment, and Poverty on Community Homeless Rates},
	volume = {27},
	issn = {1051-1482},
	url = {https://doi.org/10.1080/10511482.2017.1282885},
	doi = {10.1080/10511482.2017.1282885},
	shorttitle = {Do Local Economic Conditions Affect Homelessness?},
	abstract = {This article estimates the impact of local housing and labor market conditions on area homelessness using the U.S. Department of Housing and Urban Development's ({HUD}’s) annual point-in-time counts of homelessness from 2007 to 2014. In cross-sectional models, the median rent, the share of households in rental housing, and the poverty rate have strong positive impacts on homelessness. Once area-fixed effects are included, only the median rent remains positive and significant. However, fixed-effect models find a positive relationship between poverty and homelessness in communities that maintain right-to-shelter policies, suggesting constraints in shelter bed supply may limit responses of homelessness to changes in economic conditions.},
	pages = {640--655},
	number = {4},
	journaltitle = {Housing Policy Debate},
	author = {Hanratty, Maria},
	urldate = {2024-04-02},
	date = {2017-07-04},
	note = {Publisher: Routledge
\_eprint: https://doi.org/10.1080/10511482.2017.1282885},
	keywords = {Homeless, income, labor market, low-income housing},
}

@article{oflaherty_homelessness_2019,
	title = {Homelessness research: A guide for economists (and friends)},
	volume = {44},
	issn = {1051-1377},
	url = {https://www.sciencedirect.com/science/article/pii/S1051137718302109},
	doi = {10.1016/j.jhe.2019.01.003},
	shorttitle = {Homelessness research},
	abstract = {This paper surveys research on homelessness. The emphases are on the last decade, rather than earlier; and the United States, rather than the rest of the world. The approach is more idiosyncratic than encyclopedic. The field has been converging on the conclusion that housing subsidies are the most attractive policy for reducing homelessness, but the optimal arrangement of subsidies is not obvious. Economists have much to contribute, in both theory—mechanism design and general equilibrium—and empirics. Aggregate-level studies can be used more for finding out what the effects of different policies are, and individual-level studies can be used more for assessing the costs and benefits of those effects.},
	pages = {1--25},
	journaltitle = {Journal of Housing Economics},
