@article{edwards_latin_2019,
	title = {On Latin American Populism, and Its Echoes around the World},
	volume = {33},
	issn = {0895-3309},
	url = {https://pubs.aeaweb.org/doi/10.1257/jep.33.4.76},
	doi = {10.1257/jep.33.4.76},
	abstract = {In this article, I discuss the ways in which populist experiments have evolved historically. Populists are charismatic leaders who use a fiery rhetoric to pitch the interests of “the people” against those of banks, large firms, multinational companies, the International Monetary Fund, and immigrants. Populists implement redistributive policies that violate the basic laws of economics, and in particular budget constraints. Most populist experiments go through five distinct phases that span from euphoria to collapse. Historically, the vast majority of populist episodes end up badly; incomes of the poor and middle class tend to be lower than when the experiment was launched. I argue that many of the characteristics of traditional Latin American populism are present in more recent manifestations from around the globe.},
	pages = {76--99},
	number = {4},
	journaltitle = {Journal of Economic Perspectives},
	shortjournal = {Journal of Economic Perspectives},
	author = {Edwards, Sebastian},
	urldate = {2023-03-13},
	date = {2019-11-01},
	langid = {english},
}

