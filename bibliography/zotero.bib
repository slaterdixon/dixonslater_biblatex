@misc{simana_genetic_2023,
	title = {Genetic Fiduciaries, Rethinking Genome Governance},
	abstract = {Exploitative practices permeate the “genetic sphere” of biomedicine and biotechnology. Entities such as genetic testing and biotech companies, laboratories, biobanks, research institutions, and government agencies employ various practices to extract as much value as possible from genetic material and information. Exploitative practices include requiring individuals to waive property rights over genetic material and information; using these resources for purposes other than those for which they were obtained without the individuals’ knowledge or understanding of the implications of such use; or collecting these resources surreptitiously without any significant oversight or accountability. Such practices steer genetic material and information toward the ends of powerful entities while undermining individuals’ property and privacy interests. They result in “appropriative harms.”

Exploitative practices cannot be surmounted within the existing legal framework. This framework is fragmented, excessively narrow, and riddled with inconsistencies. It fails to provide meaningful protection against exploitative practices and does not address the profound power asymmetries between individuals and powerful entities that collect, use, and share genetic material and information. This Article addresses this gap by laying the theoretical and regulatory groundwork for future legal reform. It creates a new statutory category of “genetic fiduciaries,” proposing to impose quasi-fiduciary duties of loyalty and care on every entity in the genetic sphere, whether public or private; enacts a new federal law with broad applicability and scope; and establishes a new federal agency with rulemaking, supervision, and enforcement responsibilities.},
	publisher = {Law and Tech Workshop},
	author = {Simana, Shelly},
	date = {2023-05},
}

@article{spronk_roots_2007,
	title = {Roots of Resistance to Urban Water Privatization in Bolivia: The "New Working Class," the Crisis of Neoliberalism, and Public Services},
	issn = {0147-5479},
	url = {https://www.jstor.org/stable/27673068},
	shorttitle = {Roots of Resistance to Urban Water Privatization in Bolivia},
	abstract = {This paper analyzes the roots of resistance to the privatization of public services in the context of the changes to class formation in Bolivia. Based upon two case studies of urban water privatization, it seeks to explain why the social coalitions that have emerged to protest the privatization of public water services in Bolivia have been led by territorially-based organizations composed of rural-urban and multiclass alliances rather than public-sector unions. It argues that protest against the privatization of water utilities in Bolivia must be understood within the context of neoliberal economic restructuring and the emergence of what has been termed the "new working class," which is now primarily urban and engaged in informal forms of work.},
	pages = {8--28},
	number = {71},
	journaltitle = {International Labor and Working-Class History},
	author = {Spronk, Susan},
	urldate = {2023-05-16},
	date = {2007},
	note = {Publisher: [International Labor and Working-Class, Inc., Cambridge University Press]},
}

