@article{mullins_tax_2004,
	title = {Tax and Expenditure Limitations and the Fiscal Response of Local Government: Asymmetric Intra-Local Fiscal Effects},
	volume = {24},
	issn = {1540-5850},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/j.0275-1100.2004.00350.x},
	doi = {10.1111/j.0275-1100.2004.00350.x},
	shorttitle = {Tax and Expenditure Limitations and the Fiscal Response of Local Government},
	abstract = {This article empirically investigates the effects of introducing statewide measures to limit the ability of local jurisdictions to raise revenue or make expenditures. It does so in intervals of five years over a 25-year period, using fiscal observations for 31,804 units of local government in 787 metropolitan counties across the contiguous United States. Specifically explored is the effect of tax and expenditure limitations on the variation in revenues and expenditures between general purpose governments and school districts within county areas. Rather than imposing a uniform constraint across jurisdictions, tax and expenditure limitations (℡s) are associated with increased variation across both general purpose and school district revenues and expenditures and, by implication, increased service differentials. Effects are found to be asymmetric, with increased variation greatest within counties comprising the urban core and those with relatively more disadvantaged populations. The implications are that ℡s are most constraining on the ability of governments serving economically less prosperous and at risk populations to meet public service needs. Such differential effects are of more than questionable merit and are the result of the application of blunt instruments to what is often an undemonstrated need. The outcome impairs both the efficiency and responsiveness of the local public sector.},
	pages = {111--147},
	number = {4},
	journaltitle = {Public Budgeting \& Finance},
	author = {Mullins, Daniel R.},
	urldate = {2023-09-21},
	date = {2004},
	langid = {english},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/j.0275-1100.2004.00350.x},
}

